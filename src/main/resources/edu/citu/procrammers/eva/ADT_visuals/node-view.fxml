<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane fx:id="stackPane" maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.citu.procrammers.eva.controllers.ADT.NodeController">
   <children>
      <Circle fx:id="circle" fill="DODGERBLUE" radius="21.0" stroke="BLACK" strokeType="INSIDE" />

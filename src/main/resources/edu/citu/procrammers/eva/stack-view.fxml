<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.citu.procrammers.eva.controllers.StackViewController">
   <center>
      <AnchorPane fx:id="apVisualizer" prefHeight="720.0" prefWidth="1280.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="200.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <items>
                  <TextField fx:id="tfPrompt" prefWidth="200.0" />
                  <Button fx:id="btnPush" mnemonicParsing="false" onAction="#onButtonClick" prefWidth="100.0" text="PUSH" />
                  <Button fx:id="btnPop" mnemonicParsing="false" onAction="#onButtonClick" prefWidth="100.0" text="POP" />
                  <Button fx:id="btnTop" mnemonicParsing="false" onAction="#onButtonClick" prefWidth="100.0" text="TOP" />
                  <Button fx:id="btnClear" mnemonicParsing="false" onAction="#onButtonClick" prefWidth="100.0" text="CLEAR" />
               </items>
            </ToolBar>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
